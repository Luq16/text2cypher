    "statistics": {
      "total_nodes": "600M+",
      "total_relationships": "600M+",
      "drugs": 17000,
      "diseases": 94000,
      "genes": 51000,
      "chemicals": 49000,
      "causal_relationships": 1480000
    }
  },
  "node_types": [
    {
      "label": "Drug",
